{
  "name": "uArmStudio",
  "version": "1.1.17",
  "minimumFirmware": "3.2.0",
  "main": "./dist/main.js",
  "description": "uArm Studio",
  "private": true,
  "homepage": "http://ufactory.cc",
  "copyright": "@2018 UFACTORY",
  "socialUrl": {
    "forum": "https://forum.ufactory.cc",
    "github": "https://github.com/uArm-Developer",
    "facebook": "https://www.facebook.com/groups/1653721141585397"
  },
  "issueUrl": {
    "cn": "https://www.ufactory.cc/#/cn/ticket",
    "en": "https://www.ufactory.cc/#/en/ticket"
  },
  "faq": "http://issue.ufactory.cc/faq.html",
  "remoteUrl": {
    "pro": "http://update.ufactory.cc/releases/studio",
    "beta": "http://beta.ufactory.cc/releases/studio",
    "dev": "http://uarm.studio/releases/studio"
  },
  "scripts": {
    "e2e": "npm run pack && mocha test/e2e",
    "test": "npm run unit && npm run e2e",
    "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "dev": "node config/runner.js",
    "pack:main": "cross-env BABEL_ENV=main webpack -p --progress --colors --config config/webpack.main.config.js",
    "pack:renderer": "webpack -p --progress --colors --config config/webpack.renderer.config.js",
    "pack:beta": "cross-env NODE_ENV=production STUDIO_CHANNEL=beta npm run pack:main && cross-env NODE_ENV=production STUDIO_CHANNEL=beta npm run pack:renderer",
    "build:beta": "cross-env STUDIO_CHANNEL=beta build --config config/builder/beta.json",
    "publish:beta": "publish -c config/publisher/beta.json -p version=$npm_package_version -p remoteUrl=$npm_package_remoteUrl_beta -p minimumFirmware=$npm_package_minimumFirmware -t qiniu",
    "release:beta": "npm run pack:beta && npm run build:beta -- -mwl && npm run publish:beta -t qiniu",
    "pack:prod": "cross-env NODE_ENV=production STUDIO_CHANNEL=production npm run pack:main && cross-env NODE_ENV=production STUDIO_CHANNEL=production npm run pack:renderer",
    "build:prod": "cross-env STUDIO_CHANNEL=production build --config config/builder/production.json",
    "publish:prod": "publish -c config/publisher/prod.json -p version=$npm_package_version -p remoteUrl=$npm_package_remoteUrl_pro -p minimumFirmware=$npm_package_minimumFirmware -t qiniu",
    "release:prod": "npm run pack:prod && npm run build:prod -- -mwl && npm run publish:prod -t qiniu",
    "publish:test-beta": "publish -c config/publisher/beta.json -p version=$npm_package_version -p remoteUrl=$npm_package_remoteUrl_dev -p minimumFirmware=$npm_package_minimumFirmware -t ssh",
    "release:test-beta": "npm run pack:beta && npm run build:beta -- -mwl && npm run publish:test-beta",
    "publish:test-prod": "publish -c config/publisher/prod.json -p version=$npm_package_version -p remoteUrl=$npm_package_remoteUrl_dev -p minimumFirmware=$npm_package_minimumFirmware -t ssh",
    "release:test-prod": "npm run pack:pro && npm run build:pro -- -mwl && npm run publish:test-pro"
  },
  "author": "UFACTORY <developer@ufactory.cc>",
